{
  "name": "vue3-bulma-template",
  "version": "0.0.0",
  "scripts": {
    "serve": "npm run css-build && npm run serve:app",
    "serve:app": "npx vite --port 8080",
    "preview": "npx vite preview --port 8080",
    "build": "npx vite build",
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss css/mystyles.css",
    "deploy": "npx simple-github-pages-deploy"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@mdi/font": "^6.6.96",
    "simpler-fetch": "^6.1.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "bulma": "^0.9.3",
    "node-sass": "^7.0.1",
    "simple-github-pages-deploy": "^0.2.1",
    "vite": "^2.9.1"
  },
  "description": "A simple to use frontend template based on vue3 and bulma.",
  "main": "vite.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessicajacelyn/beep.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jessicajacelyn/beep/issues"
  },
  "homepage": "https://github.com/jessicajacelyn/beep#readme"
}
